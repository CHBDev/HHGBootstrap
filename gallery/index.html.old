          <script>
          console.log("jquery loaded");
          $(document).ready(function(){
            var colorPicker = ["#111111", "#000000", "#110000", "#090909", "#151515","#000000", "#190009", "#190900", "#191105", "#222222", "#161616" ];
            var colorPicker2 = ["#ffffff", "#ffffff", "#ffffff", "#ff6644", "#cccccc","#aaaaaa", "#ffffff", "#ffffff", "#gggggg", "#aaaaaa", "#bbbbbb" ];
            var $theBody, $aDiv, topPos, leftPos;
            $theBody = $(".hmm");

            var tWidth = 1920;
            var tHeight = 1080;
            var tSide = Math.floor( 1920 / 60);

            function colorRandom(){
              return Math.floor((Math.random() * colorPicker.length - 1) + 1);
            }

            function changeBlock(){
              if(Math.random() > .7){

                return true;
              }

              return false;
            }

            function randomTime(){
              return (Math.random() * (2.0 - .5) + .5) * 1000;
            }

            function doNewColorLoop(){
              var arr = $(".backblock");

              for(var i = 0; i < arr.length; i++){
                if(changeBlock() === true){
                  $(arr[i]).css("background-color", colorPicker[colorRandom()]);
                }
              }
              return setTimeout(doNewColorLoop, randomTime());
            }

            function doNewColorMessageLoop(){
              var arr = $(".messageicon");

              for(var i = 0; i < arr.length; i++){
                $(arr[i]).css("color", colorPicker2[colorRandom()]);
              }

              setTimeout(doNewColorMessageLoop, randomTime());

            }

            for(var x = 0; x < 30; x++){

              for(var y = 0; y < 20; y++){

                topPos = y * tSide;
                leftPos = x * tSide;
                $aDiv = $("<div class='backblock' id='" + x + y + "'></div>");
                $aDiv.css({
                  "background-color": colorPicker[colorRandom()],
                  display: "inline-block",
                  height: "" + (tSide - 1),
                  width: "" + (tSide - 1),
                  "vertical-align":"center",
                  "z-index": -1,

                  position: "absolute",
                  top: "" + topPos + "px",
                  left: "" + leftPos + "px",
                  transition: "background-color 2.2s",

                });
                $theBody.append($aDiv);

              }

            }

            doNewColorLoop();
            doNewColorMessageLoop();

          });
</script>

